{
	"name": "Automatad : Script Running Tool",
	"short_name": "Automatad",
	"version": "0.1.1",
	"manifest_version": 2,
	"description": "Download the plugin to run bidding information",
	"browser_action": {
		"default_popup": "popup.html"
	  },
		"permissions": [
		"activeTab",
		"tabs",
		"debugger",
		"webNavigation",
		"http://*/*",
		"https://*/*",
		"notifications"
	],
	"content_scripts": [
		{
			"matches":["https://*/*"],
		  "js": ["content.js"]
		}
	  ]
}
